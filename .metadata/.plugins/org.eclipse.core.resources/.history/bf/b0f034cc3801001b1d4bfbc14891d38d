package practice;
import java.util.*;
public class Main {
	//↑, ↖, ←, ↙, ↓, ↘, →, ↗
	static int[][] map = new int[5][5];
	static int[][] dir = new int[5][5];
	static int[] xDir = {-1,-1,0,1,1,1,0,-1};
	static int[] yDir = {0,-1,-1,-1,0,1,1,1};
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		for(int i=1;i<=4;i++) {
			for(int j=1;j<=8;j++) {
				if(j%2==0) {
					map[i][j/2+1] = sc.nextInt();
				}else {
					dir[i][j/2+1] = sc.nextInt();
				}// if~else end
			}// for j end
		}// for i end
		
		
		for(int i=1;i<=4;i++) {
			for(int j=1; j<=4;j++) {
				System.out.println(map[i][j] + " ");
			}// for j end
			System.out.println();
		}// for i end
		for(int i=1;i<=4;i++) {
			for(int j=1; j<=4;j++) {
				System.out.println(dir[i][j] + " ");
			}// for j end
			System.out.println();
		}// for i end
		
		
		
	}// main() end

}// Main end
