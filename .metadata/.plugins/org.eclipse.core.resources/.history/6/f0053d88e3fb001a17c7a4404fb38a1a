package control;

import java.util.Scanner;

public class BreakEx1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * 		break 구문만나면 반복문을 멈춤
		 * */
		for(int i=0;i<1000;i++) {
			System.out.println(i);
			if(i>10) {
				break;
			}
		}//for end
	}//main() end
	
}//WhileEx3 end
