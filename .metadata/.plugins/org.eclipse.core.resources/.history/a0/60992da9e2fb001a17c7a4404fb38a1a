package control;

import java.util.Scanner;

public class BreakEx1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		long l = 0;
		for(int i=0;i<1000000000;i++) {
			l+=i;
		}//for end
		System.out.println(l);
	}//main() end
	
}//WhileEx3 end
